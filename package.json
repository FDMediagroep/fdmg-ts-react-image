{
  "name": "@fdmg/ts-react-image",
  "version": "1.0.6",
  "description": "ReactJS Image component",
  "main": "dist/Image.js",
  "types": "dist/Image.d.ts",
  "scripts": {
    "release": "npm update && npm install && yarn && npm run test && npm run lint && tsc -d",
    "test": "jest",
    "lint": "tslint -p .",
    "updateSnapshot": "jest --updateSnapshot",
    "u": "npm test -- -u"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:FDMediagroep/fdmg-ts-react-image.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Willem Liu, Alexander Eerenberg",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "@types/react": "^16.3.13",
    "coveralls": "^3.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.4.0",
    "enzyme-to-json": "^3.3.3",
    "fs-extra": "^7.0.0",
    "jest": "^22.0.1",
    "jest-cli": "^23.0.0",
    "raf": "^3.4.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-test-renderer": "^16.7.0",
    "ts-jest": "23.10.5",
    "tslint": "^5.9.1",
    "tslint-react": "^3.5.1",
    "typescript": "^2.8.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "setupFiles": [
      "raf/polyfill"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "collectCoverage": true,
    "coverageDirectory": "dist/coverage",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ],
    "mapCoverage": true
  },
  "dependencies": {}
}
